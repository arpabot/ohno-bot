// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Synthesizer {
  userId String @id
  speed Float
  pitch Float
  voice String
}

model Dictionary {
  id Int @id @default(autoincrement())
  guildId String
  word String @unique
  read String
}